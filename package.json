{
  "name": "typescript-electron-exam-app",
  "version": "10.0.0",
  "description": "Electron Exam App for Remote Testing",
  "keywords": [
    "electron",
    "exam",
    "webrtc",
    "Remote",
    "Testing"
  ],
  "author": {
    "name": "SukbeomH",
    "email": "brent93.dev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SukbeomH/typescriptElectron-examapp"
  },
  "license": "MIT",
  "main": "typescript/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "yarn build && electron ./typescript/main.js",
    "package": "yarn build && electron-builder build -wm --config ./src/config.js",
    "dist": "yarn build && electron-builder -wm --publish onTagOrDraft"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.4",
    "@types/node": "^20.3.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "dotenv": "^16.3.1",
    "electron": "^24.3.0",
    "electron-builder": "^24.4.0",
    "electron-packager": "^17.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "electron-updater": "^6.1.1",
    "find-process": "^1.4.7"
  }
}
